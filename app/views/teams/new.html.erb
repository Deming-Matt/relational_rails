<%= form_with url: '/teams', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :roster_spots %>
  <%= form.text_field :roster_spots %>
  <%= form.label :full_roster %>
  <%= form.text_field :full_roster %>

  <%= form.submit "Create New Team" %>
<% end %>
