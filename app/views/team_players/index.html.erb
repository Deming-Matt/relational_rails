<h1>"<%= @teams.name %> Roster"</h1>
<%= link_to "Teams", "/teams" %> <%= link_to "All Players", "/players" %>

<% @players.each do |player| %>
<p>"Name: <%= player.fname %> <%= player.lname %>, Jersey: <%= player.jersey %>, Healthy: <%= player.healthy %>"</p>
<% end %>


<%= link_to "Add Player", "/teams/#{@teams.id}/players/new" %>
<%= link_to "Alphabetize", "/teams/#{@teams.id}/players/alphabetize" %>
