<%= form_with url: "/players/#{@player_id}", method: :patch, local: true do |form| %>
<%= form.label :first_name %>
<%= form.text_field :fname %>
<%= form.label :last_name %>
<%= form.text_field :lname %>
<%= form.label :jersey %>
<%= form.text_field :jersey %>
<%= form.label :healthy %>
<%= form.text_field :healthy %>

<%= form.submit "Update" %>
<% end %>
